{
  "name": "vite-plugin-vue-setup-name",
  "version": "0.0.4",
  "description": "Make the vue script setup syntax support the name attribute",
  "homepage": "https://github.com/lhlyu/vite-plugin-vue-setup-name",
  "bugs": {
    "url": "https://github.com/lhlyu/vite-plugin-vue-setup-name/issues"
  },
  "license": "MIT",
  "author": "lhlyu",
  "repository": "https://github.com/lhlyu/vite-plugin-vue-setup-name.git",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
