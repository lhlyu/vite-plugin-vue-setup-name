    "dev": "unbuild --stub",
    "build": "unbuild"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.5.27",
    "magic-string": "^0.30.21"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.3.1"
  },
  "peerDependencies": {
    "vite": "^7.0.0"
  }
